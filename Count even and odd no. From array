#include<stdio.h>
int main()
{
        int i,n,c1=0,c2=0,a[100];
        printf("enter limit :");
        scanf("%d",&n);
        printf("enter n numbers :");
        for(i=0;i<n;i++)
        {
        scanf("%d",&a[i]);
        }
        for(i=0;i<n;i++)
        {
        if(a[i]%2==0)
        c1++;

        else

        c2++;
        }
        printf("\n even count of occurance=%d",c1);
        printf("\n odd count of occurance=%d",c2);
}
