#include<stdio.h>
int main()
{
        int i,n,sum=0,a[100];
        printf("enter limit:");
        scanf("%d",&n);
        printf("enter n numbers:");
        for(i=0;i<n;i++)
        {
        scanf("%d",&a[i]);
        }
        for(i=0;i<n;i++)
        {
        sum=sum+a[i];
        }
        printf("sum of numbers=%d",sum);
        }
