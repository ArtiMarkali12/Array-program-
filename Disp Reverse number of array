#include<stdio.h>
int main()
{
        int i,n;
        float a[100];
        printf("enter limit:");
        scanf("%d",&n);
        printf("enter n numbers :");
        for(i=0;i<n;i++)
        {
        scanf("%f",&a[i]);
        }
        printf("array in revrese :");
        for(i=n-1;i>=0;i--)
        {
        printf("%.2f\n",a[i]);
        }
}
