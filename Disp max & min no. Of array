#include<stdio.h>
int main()
{
        int i,n,a[100],max,min;
        printf("enter limit :");
        scanf("%d",&n);
        printf("enter n numbers :");
        for(i=0;i<n;i++)
        scanf("%d",&a[i]);
        min=a[0];max=a[0];
        for(i=0;i<n;i++)
        {
        if(a[i]>max)
        max=a[i];
        if(a[i]<min)
        min=a[i];
        }
        printf("\n maximum no =%d",max);
        printf("\n minimum no =%d",min);
        }
