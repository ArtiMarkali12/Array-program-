#include<stdio.h>
int main()
{
        int i,n,a[100],max;
        printf("enter limit :");
        scanf("%d",&n);
        printf("enter n numbers :");
        for(i=0;i<n;i++)
        {
        scanf("%d",&a[i]);
        }
        for(i=0;i<n;i++)
        {
        if(a[i]>max)
        max=a[i];
        }
        printf("maximum no =%d",max);
        }
